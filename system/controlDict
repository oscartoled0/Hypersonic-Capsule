FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoCentralFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.0015;

deltaT          1e-8;

writeControl    runTime;

writeInterval   0.00001;

purgeWrite      0;

writeFormat    	ascii;

writePrecision  7;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxC0		0.5;

maxAlphaCo	0.5;

maxDeltaT	5e-7;


